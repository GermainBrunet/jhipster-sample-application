enum Language {
    FRENCH, ENGLISH, SPANISH
}

entity LanguageValue {
	language Language,
    value String required
}

entity LanguageCode {
	labelLanguage LabelLanguage,
	code String required
}

relationship OneToMany {
    LanguageCode{code} to LanguageValue{languageCode(code)}
}

paginate LanguageValue with pagination
dto * with mapstruct
service all with serviceImpl